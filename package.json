{
  "name": "simple-digital-clock-widget",
  "version": "0.5.1",
  "description": "Simple Digital Clock Widget html",
  "scripts": {
    "build": "NODE_ENV=production parcel build src/SimpleDigitalClockWidget.tsx --log-level verbose && cp dist/SimpleDigitalClockWidget.js dist/latest.min.js && cp dist/SimpleDigitalClockWidget.js dist/$npm_package_version.min.js",
    "start": "parcel src/index.html --open",
    "prettier": "prettier --write ./src",
    "lint": "eslint 'src/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/*.{ts,tsx}' --fix",
    "test": "tsc && wtr src/*.test.{ts,tsx}",
    "test:browsers": "tsc && wtr src/*.test.{ts,tsx} --playwright --browsers chromium firefox webkit"
  },
  "keywords": [],
  "author": "Timenow.zone",
  "license": "MIT",
  "dependencies": {
    "clsx": "^2.0.0",
    "preact": "^10.18.2",
    "preact-custom-element": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@parcel/compressor-brotli": "^2.10.2",
    "@parcel/compressor-gzip": "^2.10.2",
    "@parcel/config-default": "^2.10.2",
    "@parcel/transformer-inline-string": "2.10.2",
    "@parcel/transformer-yaml": "2.10.2",
    "@parcel/validator-typescript": "^2.10.2",
    "@types/node": "^20.8.10",
    "@types/preact-custom-element": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "parcel": "^2.10.2",
    "postcss": "^8.4.31",
    "preact-render-to-string": "^6.2.2",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "terser": "^5.24.0",
    "typescript": "^5.2.2"
  },
  "targets": {
    "default": {
      "context": "browser",
      "includeNodeModules": true,
      "distDir": "./dist",
      "isLibrary": false,
      "engines": {
        "browsers": "> 0.25%, not dead",
        "node": ">= 12"
      },
      "optimize": true
    }
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ]
}
